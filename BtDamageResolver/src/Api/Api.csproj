<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Faemiyah.BtDamageResolver.Api</RootNamespace>
    <AssemblyName>Faemiyah.BtDamageResolver.Api</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.249</Version>
    <PackageProjectUrl>https://github.com/Warmag2/BtDamageResolver</PackageProjectUrl>
    <Authors>Warma</Authors>
    <Company>Faemiyah</Company>
    <Product>BtDamageResolver</Product>
    <Copyright>Faemiyah 2020</Copyright>
    <RepositoryUrl>https://github.com/Warmag2/BtDamageResolver</RepositoryUrl>
    <Description>API classes and interfaces for BtDamageResolver. Needed to connect to and interface with a cluster.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="SevenZip.Compression.Lzma" Version="0.0.4" />
    <PackageReference Include="StackExchange.Redis" Version="2.2.50" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
  </ItemGroup>

</Project>
