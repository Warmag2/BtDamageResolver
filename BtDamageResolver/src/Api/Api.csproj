<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Faemiyah.BtDamageResolver.Api</RootNamespace>
    <AssemblyName>Faemiyah.BtDamageResolver.Api</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.218</Version>
    <PackageProjectUrl>https://github.com/Warmag2/BtDamageResolver</PackageProjectUrl>
    <Authors>Warma</Authors>
    <Company>Faemiyah</Company>
    <Product>BtDamageResolver</Product>
    <Copyright>Faemiyah 2020</Copyright>
    <RepositoryUrl>https://github.com/Warmag2/BtDamageResolver</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Orleans.CodeGenerator.MSBuild" Version="3.4.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Orleans.Core.Abstractions" Version="3.4.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="SevenZip.Compression.Lzma" Version="0.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Orleans_CodeGenInputs Remove="Entities\LoginState.cs" />
  </ItemGroup>

</Project>
